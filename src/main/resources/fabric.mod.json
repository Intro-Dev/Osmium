{
  "schemaVersion": 1,
  "id": "osmium",
  "version": "${version}",

  "name": "Osmium",
  "description": "Osmium adds various QOL features to Minecraft!",
  "authors": [
    "Intro"
  ],
  "contact": {
    "homepage": "https://github.com/Intro-Dev/Osmium",
    "sources": "https://github.com/Intro-Dev/Osmium"
  },

  "license": "CC0-1.0",
  "icon": "assets/osmium/icon.png",

  "environment": "*",
  "entrypoints": {
    "client": [
      "dev.lobstershack.client.OsmiumClient"
    ]
  },
  "accessWidener" : "osmium.accesswidener",
  "mixins": [
    "osmium.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.8.0",
    "fabric": "*",
    "minecraft": [
      "1.20",
      "1.20.1"
    ]
  },


  "custom": {
    "modmenu": {
        "id": "osmium",
        "name": "Osmium",
        "description": "Osmium adds various QOL features to Minecraft!",
        "icon": "assets/osmium/icon.png"
    }
  }
}
